<Window x:Class="Desktop.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Desktop"
        mc:Ignorable="d"
        Title="MainWindow" Height="345.67" Width="206.983">
    <Grid Margin="0,0,3.2,0">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="166*"/>
            <ColumnDefinition Width="15*"/>
        </Grid.ColumnDefinitions>
        <StackPanel HorizontalAlignment="Left">
            <Button Click="OnRequestTurnLedOn" Height="30">On</Button>
            <Button Click="OnRequestTurnLedOff" Height="30">Off</Button>
            <Button Click="OnRequestBlinkLed" Height="30">Blink</Button>
        </StackPanel>
    </Grid>
</Window>
